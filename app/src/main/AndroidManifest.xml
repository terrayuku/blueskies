<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="skies.blue.liberty.com.blueskies">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher__96x96"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login" android:parentActivityName=".MainActivity">

        </activity>
        <activity android:name=".Registration" android:parentActivityName=".MainActivity">

        </activity>
        <activity android:name=".Landing" android:parentActivityName=".Login">

        </activity>
        <activity android:name=".DesignYourBestLife" android:parentActivityName=".Landing">

        </activity>
        <activity android:name=".Profile" android:parentActivityName=".DesignYourBestLife">

        </activity>
        <activity android:name=".Continue" android:parentActivityName=".Profile">

        </activity>
        <activity android:name=".BestLife" android:parentActivityName=".Continue">

        </activity>

        <meta-data android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id"/>

        <activity android:name=".FacebookLogin"
            android:configChanges=
                "keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/app_name" />

        <activity
            android:name="skies.blue.liberty.com.blueskies.FacebookCustomTab"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data android:scheme="@string/fb_login_protocol_scheme" />
            </intent-filter>
        </activity>

    </application>

</manifest>